# Qdrant settings
QDRANT_URL=http://127.0.0.1:6333
QDRANT_COLLECTION=chat_messages

# SQLite database (SQLAlchemy URI)
SQLITE_PATH=sqlite:///./messages.db

# Embedding model for sentence-transformers
EMBEDDING_MODEL=all-MiniLM-L6-v2

# Default generation backend
VECTOR_SERVICE_DEFAULT_BACKEND=ollama

# Provider API keys / project identifiers
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GOOGLE_API_KEY=
GOOGLE_SEARCH_API_KEY=
GOOGLE_SEARCH_CX=
HUGGINGFACE_API_TOKEN=
GCP_PROJECT=
GCP_LOCATION=us-central1

# Local backends
OLLAMA_URL=http://127.0.0.1:11434
